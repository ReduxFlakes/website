---
pagination: 
    data: collections
    size: 1
    alias: tag
permalink: "blog/tagged/{{ tag | slugify }}/"
eleventyComputed:
    title: "tagged with {{ tag }}"
---

<a href="/blog">Go back to the blog</a>

<ol>
    {% set taglist = collections[tag] %}
    {% for post in taglist | reverse %}
        <li>
            <a href="{{ post.url }}">{{ post.data.title }}</a>
        </li>
    {% endfor %}
</ol>